* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

:root {
    /* Dark Theme (Default) */
    --bg-primary: #0f172a;
    --bg-secondary: #1e293b;
    --bg-tertiary: #162a45;
    --bg-card: rgba(30, 41, 59, 0.6);
    --bg-card-hover: rgba(30, 41, 59, 0.8);
    --bg-navbar: rgba(15, 23, 42, 0.8);
    --bg-modal: rgba(30, 41, 59, 0.95);
    
    --text-primary: #00ffff;
    --text-secondary: #cbd5e1;
    --text-muted: #64b5f6;
    
    --border-primary: rgba(0, 255, 255, 0.2);
    --border-hover: rgba(0, 255, 255, 0.6);
    
    --glow-primary: rgba(0, 255, 255, 0.3);
    --glow-secondary: rgba(0, 255, 255, 0.1);
    
    --gradient-1: rgba(0, 255, 255, 0.1);
    --gradient-2: rgba(100, 150, 255, 0.05);
}

body.light-theme {
    /* Light Theme */
    --bg-primary: #f0f4f8;
    --bg-secondary: #ffffff;
    --bg-tertiary: #e2e8f0;
    --bg-card: rgba(255, 255, 255, 0.9);
    --bg-card-hover: rgba(255, 255, 255, 1);
    --bg-navbar: rgba(255, 255, 255, 0.95);
    --bg-modal: rgba(255, 255, 255, 0.98);
    
    --text-primary: #0066cc;
    --text-secondary: #1e293b;
    --text-muted: #475569;
    
    --border-primary: rgba(0, 102, 204, 0.2);
    --border-hover: rgba(0, 102, 204, 0.6);
    
    --glow-primary: rgba(0, 102, 204, 0.2);
    --glow-secondary: rgba(0, 102, 204, 0.05);
    
    --gradient-1: rgba(0, 102, 204, 0.08);
    --gradient-2: rgba(100, 150, 255, 0.05);
}

/* Quote loader */
.quote-loader {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(135deg, var(--bg-primary) 0%, var(--bg-secondary) 50%, var(--bg-tertiary) 100%);
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 9999;
    animation: fadeOut 0.5s ease 5s forwards;
    pointer-events: none;
    overflow: hidden;
    transition: background 0.3s ease;
}

.quote-loader::before {
    content: '';
    position: absolute;
    width: 500px;
    height: 500px;
    background: radial-gradient(circle, var(--glow-secondary) 0%, transparent 70%);
    top: -150px;
    left: -150px;
    animation: orbitSlow 15s linear infinite;
}

.quote-loader::after {
    content: '';
    position: absolute;
    width: 400px;
    height: 400px;
    background: radial-gradient(circle, var(--gradient-2) 0%, transparent 70%);
    bottom: -100px;
    right: -100px;
    animation: orbitFast 12s linear infinite reverse;
}

.quote-loader.hide {
    display: none;
}

